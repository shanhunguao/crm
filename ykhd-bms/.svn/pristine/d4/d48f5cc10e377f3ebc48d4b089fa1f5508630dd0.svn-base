package com.ykhd.office.domain.resp;

import java.util.List;

/**
 * 菜单权限的设置信息
 */
public class SetupMenu {

	private String name;
	private String title;
	private boolean selected;
	private List<SetupMenu> children;
	
	public String getName() {
		return name;
	}
	public SetupMenu setName(String name) {
		this.name = name;
		return this;
	}
	public String getTitle() {
		return title;
	}
	public SetupMenu setTitle(String title) {
		this.title = title;
		return this;
	}
	public boolean isSelected() {
		return selected;
	}
	public SetupMenu setSelected(boolean selected) {
		this.selected = selected;
		return this;
	}
	public List<SetupMenu> getChildren() {
		return children;
	}
	public SetupMenu setChildren(List<SetupMenu> children) {
		this.children = children;
		return this;
	}
}
