package com.ykhd.office.domain.req;

import java.util.Date;
import java.util.List;

import com.ykhd.office.domain.bean.common.PageCondition;

/**
 * 客户列表查询条件
 */
public class CustomerCondition extends PageCondition {
    private Integer id;
    /**
     * 公司名称
     */
    private String company;
    /**
     * 品牌
     */
    private String brand;
    /**
     * 行业
     */
    private String industry;
    /**
     * 领域
     */
    private String domain;
    /**
     * 联系人名称
     */
    private String linkman;

    /**
     * 联系方式
     */
    private String contact;

    /**
     * 创建时间
     */
    private Date createTime;
    /**
     * 创建人id
     */
    private Integer creator;

    /**
     * 所属人
     */
    private Integer belongUser;

    /**
     * 部门员工
     */
    private List<Integer> deptbelongUser;

    /**
     * 是否锁定（0-否，1-是，2-公海）
     */
    private Integer isLock;
    /**
     * 进入公海时间
     */
    private String highSeaTimeStart;

    private String highSeaTimeEnd;

    private String isCooperate;  //合作状态

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getCompany() {
        return company;
    }

    public void setCompany(String company) {
        this.company = company;
    }

    public String getBrand() {
        return brand;
    }

    public void setBrand(String brand) {
        this.brand = brand;
    }

    public String getIndustry() {
        return industry;
    }

    public void setIndustry(String industry) {
        this.industry = industry;
    }

    public String getDomain() {
        return domain;
    }

    public void setDomain(String domain) {
        this.domain = domain;
    }

    public String getLinkman() {
        return linkman;
    }

    public void setLinkman(String linkman) {
        this.linkman = linkman;
    }

    public String getContact() {
        return contact;
    }

    public void setContact(String contact) {
        this.contact = contact;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Integer getCreator() {
        return creator;
    }

    public void setCreator(Integer creator) {
        this.creator = creator;
    }

    public Integer getBelongUser() {
        return belongUser;
    }

    public void setBelongUser(Integer belongUser) {
        this.belongUser = belongUser;
    }

    public Integer getIsLock() {
        return isLock;
    }

    public void setIsLock(Integer isLock) {
        this.isLock = isLock;
    }

    public String getHighSeaTimeStart() {
        return highSeaTimeStart;
    }

    public void setHighSeaTimeStart(String highSeaTimeStart) {
        this.highSeaTimeStart = highSeaTimeStart;
    }

    public String getHighSeaTimeEnd() {
        return highSeaTimeEnd;
    }

    public void setHighSeaTimeEnd(String highSeaTimeEnd) {
        this.highSeaTimeEnd = highSeaTimeEnd;
    }

    public String getIsCooperate() {
        return isCooperate;
    }

    public void setIsCooperate(String isCooperate) {
        this.isCooperate = isCooperate;
    }

    public List<Integer> getDeptbelongUser() {
        return deptbelongUser;
    }

    public void setDeptbelongUser(List<Integer> deptbelongUser) {
        this.deptbelongUser = deptbelongUser;
    }
}
