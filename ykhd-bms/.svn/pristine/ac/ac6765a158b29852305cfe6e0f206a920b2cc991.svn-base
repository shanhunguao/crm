	/**员工状态*/
	private String positionStatus;
	/**试用期时长（月）*/
	private String probationDuration;
	/**转正日期*/
	private String formalDate;
	/**身份证号码*/
	private String idCardNo;
	/**出生日期*/
	private String birthday;
	/**性别*/
	private String gender;
	/**民族*/
	private String nation;
	/**住址*/
	private String address;
	/**婚姻状况*/
